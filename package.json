{
  "name": "cloudnotes-backend",
  "version": "1.0.0",
  "description": "Backend for cloudnotes react app.",
  "main": "index.js",
  "scripts": {
    "start": "run-p start:**",
    "start:lambda": "netlify-lambda serve server",
    "start:app": "cd client && npm start",
    "build": "run-p build:**",
    "build:lambda": "netlify-lambda build server",
    "build:app": "cd client && npm build",
    "deploy:draft": "netlify deploy --dir=client/build --functions=functions",
    "deploy:prod": "netlify deploy --dir=client/build --functions=functions --prod"
  },
  "author": "Naveen",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "serverless-http": "^2.3.2"
  },
  "devDependencies": {
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "netlify",
    "express",
    "create-react-app",
    "react"
  ]
}
